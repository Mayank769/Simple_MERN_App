{
  "name": "ReactAPP",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "engines": {
    "node": "10.16.3",
    "npm": "6.11.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "node app.js",
    "client": "cd client && npm start",
    "dev": "concurrently -n 'server,client' -c 'red,green'  \"nodemon server.js\" \"npm run client\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^5.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "debug": "~2.6.9",
    "ejs": "^3.1.3",
    "express": "~4.16.1",
    "history": "^5.0.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.9.21",
    "morgan": "~1.9.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  }
}
